package it.polito.oop.vaccination;

public class Giorno {
	String name;
	int ore;
	int giorno;

	
	public Giorno(int ore, int giorno) {
		this.ore = ore;
		this.giorno = giorno;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getOre() {
		return ore;
	}


	public void setOre(int ore) {
		this.ore = ore;
	}


	public int getGiorno() {
		return giorno;
	}


	public void setGiorno(int giorno) {
		this.giorno = giorno;
	}
	
	
	

}
