package it.polito.oop.zombies;

public class Director {
	
	private String name;
	private int bfoo,bar,baz;
	
	public Director(String name) {
		
		this.name = name;
	}

	public String getName() {
		return name;
	}

	

	public int getBfoo() {
		return bfoo;
	}

	public void setBfoo(int bfoo) {
		this.bfoo = bfoo;
	}

	public int getBar() {
		return bar;
	}

	public void setBar(int bar) {
		this.bar = bar;
	}

	public int getBaz() {
		return baz;
	}

	public void setBaz(int baz) {
		this.baz = baz;
	}
	
	
}
